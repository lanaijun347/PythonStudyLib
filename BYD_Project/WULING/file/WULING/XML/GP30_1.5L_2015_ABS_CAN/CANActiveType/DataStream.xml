				<command_id>DS_CMD_00_008</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>2</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*256+B)</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- 纵向加速度 -->
			<data_stream ID="9" caption="ID_STR_DS_ITEM_0009" unit="m/s^2" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_009</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>2</valid_byte_number>
				<caculate type="LJ">
					<!--公式-->
					<formula>2,1,0x0000,0x7FFF,(A*256+B)*0.015625+0,0x8000,0xFFFF,((A*256+B)-65535)*0.015625+0</formula>
				</caculate>
				<!--显示控制-->
				<format>%.6f</format>
			</data_stream>
			<!-- 横向加速度 -->
			<data_stream ID="10" caption="ID_STR_DS_ITEM_0010" unit="m/s^2" min="" max="" self_learn="0">
