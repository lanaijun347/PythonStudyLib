<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<root version="1.0">
	<function ID="ID_FUNCTION_DATASTREAM">
		<!--选择数据流，并把选择的数据流ID保存到ui_data_buffer中-->
		<show label="" type="data_stream_select">
			<param type="function" value="ID_FUNCTION_READ_DATASTREAM"/>
			<param label="data_stream_id" type="ui_data_buffer" value=""/>
			<param label="GROUP" type="string" value="0,1,2,3,4,5"/>
		</show>
	</function>
	<function ID="ID_FUNCTION_READ_DATASTREAM">
		<call_protocol label="data_stream" function="read_data_stream">
			<param type="ui_data_buffer" value="data_stream_id"/>
		</call_protocol>
		<judge_tip_jump use_label="data_stream"/>
		<show label="" type="read_data_stream">
			<param type="protocol_data" value="data_stream"/>
		</show>
	</function>
	<data_stream_lib>
		<!-- LAM供电电源 -->
		<group ID="0" caption="ID_STR_DS_GROUP_00">
			<!-- LAM供电电压 -->
			<data_stream ID="0" caption="ID_STR_DS_ITEM_0000" unit="V" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_000</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>A*0.1</formula>
				</caculate>
				<!--显示控制-->
				<format>%.1f</format>
			</data_stream>
		</group>
		<!-- 车速 -->
		<group ID="1" caption="ID_STR_DS_GROUP_01">
			<!-- 车速 -->
			<data_stream ID="1" caption="ID_STR_DS_ITEM_0001" unit="km/h" min="" max="" self_learn="0">
				<command_id>DS_CMD_01_000</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>2</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*256+B)*0.01625</formula>
				</caculate>
				<!--显示控制-->
				<format>%.5f</format>
			</data_stream>
		</group>
		<!-- 整车Ready状态 -->
		<group ID="2" caption="ID_STR_DS_GROUP_02">
			<!-- 车辆Ready状态 -->
			<data_stream ID="2" caption="ID_STR_DS_ITEM_0002" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_02_000</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X1)Y=@0106;ELSE Y=@0105;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
		</group>
		<!-- 低速警示 -->
		<group ID="3" caption="ID_STR_DS_GROUP_03">
			<!-- 低速警示间歇开关状态 -->
			<data_stream ID="3" caption="ID_STR_DS_ITEM_0003" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_03_000</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X1)Y=@0009;ELSE Y=@0008;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- 低速警示间歇指示灯状态 -->
			<data_stream ID="4" caption="ID_STR_DS_ITEM_0004" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_03_001</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X1)Y=@0009;ELSE Y=@0008;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
		</group>
		<!-- 翁鸣器 -->
		<group ID="4" caption="ID_STR_DS_GROUP_04">
			<!-- 翁鸣器状态 -->
			<data_stream ID="5" caption="ID_STR_DS_ITEM_0005" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_04_000</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X1)Y=@0009;ELSE Y=@0008;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
		</group>
		<!-- 档位状态 -->
		<group ID="5" caption="ID_STR_DS_GROUP_05">
			<!-- 钥匙插入开关 -->
			<data_stream ID="6" caption="ID_STR_DS_ITEM_0006" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_05_000</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X80)Y=@0108;ELSE Y=@0107;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- ACC档 -->
			<data_stream ID="7" caption="ID_STR_DS_ITEM_0007" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_05_000</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X40)Y=@0110;ELSE Y=@0109;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- ON档 -->
			<data_stream ID="8" caption="ID_STR_DS_ITEM_0008" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_05_000</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X20)Y=@0112;ELSE Y=@0111;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- KL50起动档 -->
			<data_stream ID="9" caption="ID_STR_DS_ITEM_0009" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_05_000</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X10)Y=@0114;ELSE Y=@0113;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
		</group>
	</data_stream_lib>
	<switchs/>
</root>
