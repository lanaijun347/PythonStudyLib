<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<root version="1.0">
	<function ID="ID_FUNCTION_DATASTREAM">
		<!--选择数据流，并把选择的数据流ID保存到ui_data_buffer中-->
		<show label="" type="data_stream_select">
			<param type="function" value="ID_FUNCTION_READ_DATASTREAM"/>
			<param label="data_stream_id" type="ui_data_buffer" value=""/>
			<param label="GROUP" type="string" value="0"/>
		</show>
	</function>
	<function ID="ID_FUNCTION_READ_DATASTREAM">
		<call_protocol label="data_stream" function="read_data_stream">
			<param type="ui_data_buffer" value="data_stream_id"/>
		</call_protocol>
		<judge_tip_jump use_label="data_stream"/>
		<show label="" type="read_data_stream">
			<param type="protocol_data" value="data_stream"/>
		</show>
	</function>
	<data_stream_lib>
		<!-- 数据流 -->
		<group ID="0" caption="ID_STR_DS_GROUP_00">
			<!-- Vehicle speed -->
			<data_stream ID="0" caption="ID_STR_DS_ITEM_0000" unit="Km/h" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_000</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>A</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- Igntion KEY position -->
			<data_stream ID="1" caption="ID_STR_DS_ITEM_0001" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_001</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>switch(0,A)</formula>
				</caculate>
				<!--显示控制-->
				<format>STRING</format>
			</data_stream>
			<!-- AVM system calibration Status -->
			<data_stream ID="2" caption="ID_STR_DS_ITEM_0002" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_002</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>switch(1,A)</formula>
				</caculate>
				<!--显示控制-->
				<format>STRING</format>
			</data_stream>
			<!-- Reverse switch signal -->
			<data_stream ID="3" caption="ID_STR_DS_ITEM_0003" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_003</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>switch(2,A)</formula>
				</caculate>
				<!--显示控制-->
				<format>STRING</format>
			</data_stream>
			<!-- Steering Wheel Angle signal -->
			<data_stream ID="4" caption="ID_STR_DS_ITEM_0004" unit="deg" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_004</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>2</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*256+B)*0.0625-2048</formula>
				</caculate>
				<!--显示控制-->
				<format>%.4f</format>
			</data_stream>
			<!-- 360DV camera switch status -->
			<data_stream ID="5" caption="ID_STR_DS_ITEM_0005" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_005</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>switch(2,A)</formula>
				</caculate>
				<!--显示控制-->
				<format>STRING</format>
			</data_stream>
			<!-- Car color -->
			<data_stream ID="6" caption="ID_STR_DS_ITEM_0006" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_006</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>switch(3,A)</formula>
				</caculate>
				<!--显示控制-->
				<format>STRING</format>
			</data_stream>
			<!-- AVM Output View Status -->
			<data_stream ID="7" caption="ID_STR_DS_ITEM_0007" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_007</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>switch(4,A)</formula>
				</caculate>
				<!--显示控制-->
				<format>STRING</format>
			</data_stream>
			<!-- Enquiry mode -->
			<data_stream ID="8" caption="ID_STR_DS_ITEM_0008" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_008</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>switch(1,A)</formula>
				</caculate>
				<!--显示控制-->
				<format>STRING</format>
			</data_stream>
			<!-- Enquiry AVM vol -->
			<data_stream ID="9" caption="ID_STR_DS_ITEM_0009" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_009</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>A*0.1</formula>
				</caculate>
				<!--显示控制-->
				<format>%.1f</format>
			</data_stream>
			<!-- Front View Dynamic Guides -->
			<data_stream ID="10" caption="ID_STR_DS_ITEM_0010" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_010</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X80)Y=@0009;ELSE Y=@0008;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- Back View Dynamic Guides -->
			<data_stream ID="11" caption="ID_STR_DS_ITEM_0011" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_010</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X40)Y=@0009;ELSE Y=@0008;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
		</group>
	</data_stream_lib>
	<switchs>
		<!-- 00:Lock 01:ACC 02:RUN 03:START 其他:-  -->
		<switch ID="0">
			<case value="0x00">ID_DS_STRING_000</case>
			<case value="0x01">ID_DS_STRING_001</case>
			<case value="0x02">ID_DS_STRING_002</case>
			<case value="0x03">ID_DS_STRING_003</case>
			<default>ID_DS_STRING_004</default>
		</switch>
		<!-- 00:Calibration is failed 01:Calibration result is saving 02:Calibration is finished (calibration result is saved successfully) 其他:-  -->
		<switch ID="1">
			<case value="0x00">ID_DS_STRING_005</case>
			<case value="0x01">ID_DS_STRING_006</case>
			<case value="0x02">ID_DS_STRING_007</case>
			<default>ID_DS_STRING_004</default>
		</switch>
		<!-- 00:inactive 01:active 其他:-  -->
		<switch ID="2">
			<case value="0x00">ID_DS_STRING_008</case>
			<case value="0x01">ID_DS_STRING_009</case>
			<default>ID_DS_STRING_004</default>
		</switch>
		<!-- 00:Not set 01:Starry black 02:Burgundy red 03:Pearl white 04:Aurora silver 05:Crystal brown 06:Starry blue 其他:-  -->
		<switch ID="3">
			<case value="0x00">ID_DS_STRING_010</case>
			<case value="0x01">ID_DS_STRING_011</case>
			<case value="0x02">ID_DS_STRING_012</case>
			<case value="0x03">ID_DS_STRING_013</case>
			<case value="0x04">ID_DS_STRING_014</case>
			<case value="0x05">ID_DS_STRING_015</case>
			<case value="0x06">ID_DS_STRING_016</case>
			<default>ID_DS_STRING_004</default>
		</switch>
		<!-- 00:Exit 01:Open AVM 02:Enter Calibration 03:Update AVM 其他:-  -->
		<switch ID="4">
			<case value="0x00">ID_DS_STRING_017</case>
			<case value="0x01">ID_DS_STRING_018</case>
			<case value="0x02">ID_DS_STRING_019</case>
			<case value="0x03">ID_DS_STRING_020</case>
			<default>ID_DS_STRING_004</default>
		</switch>
	</switchs>
</root>
