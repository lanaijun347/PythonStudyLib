<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<root version="1.0">
	<function ID="ID_FUNCTION_DATASTREAM">
		<!--选择数据流，并把选择的数据流ID保存到ui_data_buffer中-->
		<show label="" type="data_stream_select">
			<param type="function" value="ID_FUNCTION_READ_DATASTREAM"/>
			<param label="data_stream_id" type="ui_data_buffer" value=""/>
			<param label="GROUP" type="string" value="0"/>
		</show>
	</function>
	<function ID="ID_FUNCTION_READ_DATASTREAM">
		<call_protocol label="data_stream" function="read_data_stream">
			<param type="ui_data_buffer" value="data_stream_id"/>
		</call_protocol>
		<judge_tip_jump use_label="data_stream"/>
		<show label="" type="read_data_stream">
			<param type="protocol_data" value="data_stream"/>
		</show>
	</function>
	<data_stream_lib>
		<!-- 数据流 -->
		<group ID="0" caption="ID_STR_DS_GROUP_00">
			<!-- OBD偏移量 -->
			<data_stream ID="0" caption="ID_STR_DS_ITEM_0000" unit="km" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_000</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>A</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- 发动机转速 -->
			<data_stream ID="1" caption="ID_STR_DS_ITEM_0001" unit="rpm" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_001</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>2</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*256+B)*0.15625</formula>
				</caculate>
				<!--显示控制-->
				<format>%.5f</format>
			</data_stream>
			<!-- 里程小计A -->
			<data_stream ID="2" caption="ID_STR_DS_ITEM_0002" unit="km" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_002</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>2</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*256+B)*0.1</formula>
				</caculate>
				<!--显示控制-->
				<format>%.1f</format>
			</data_stream>
			<!-- 里程小计B -->
			<data_stream ID="3" caption="ID_STR_DS_ITEM_0003" unit="km" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_003</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>2</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*256+B)*0.1</formula>
				</caculate>
				<!--显示控制-->
				<format>%.1f</format>
			</data_stream>
			<!-- 油位 -->
			<data_stream ID="4" caption="ID_STR_DS_ITEM_0004" unit="litre" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_004</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>A</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- 发动机冷却液温度 -->
			<data_stream ID="5" caption="ID_STR_DS_ITEM_0005" unit="ºC" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_005</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>2</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*256+B)*0.75-48</formula>
				</caculate>
				<!--显示控制-->
				<format>%.2f</format>
			</data_stream>
			<!-- 冷却液温度指示灯变红 -->
			<data_stream ID="6" caption="ID_STR_DS_ITEM_0006" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_006</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X80)Y=@0120;ELSE Y=@0119;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
