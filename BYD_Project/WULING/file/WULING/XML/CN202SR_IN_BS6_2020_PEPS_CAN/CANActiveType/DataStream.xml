	<switchs>
		<!-- 00:No fail 01:Received ESK not matched with stored 02:Erased internal learning data failed 03:Update learning status failed 04:Erase ESK failed 其他:-  -->
		<switch ID="0">
			<case value="0x00">ID_DS_STRING_000</case>
			<case value="0x01">ID_DS_STRING_001</case>
			<case value="0x02">ID_DS_STRING_002</case>
			<case value="0x03">ID_DS_STRING_003</case>
			<case value="0x04">ID_DS_STRING_004</case>
			<default>ID_DS_STRING_005</default>
		</switch>
		<!-- 55:SYstem with ESCL AA:SYstem without ESCL 其他:-  -->
		<switch ID="1">
			<case value="0x55">ID_DS_STRING_006</case>
			<case value="0xAA">ID_DS_STRING_007</case>
			<default>ID_DS_STRING_005</default>
		</switch>
		<!-- 00:no blanking time 01:enable blanking for only Unlock 02:enable blanking for only Lock 03:enable blanking for unlock and lock 其他:-  -->
		<switch ID="2">
			<case value="0x00">ID_DS_STRING_008</case>
			<case value="0x01">ID_DS_STRING_009</case>
			<case value="0x02">ID_DS_STRING_010</case>
			<case value="0x03">ID_DS_STRING_011</case>
			<default>ID_DS_STRING_005</default>
		</switch>
	</switchs>
</root>
