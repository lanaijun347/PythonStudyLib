<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<root version="1.0">
	<function ID="ID_FUNCTION_DATASTREAM">
		<!--选择数据流，并把选择的数据流ID保存到ui_data_buffer中-->
		<show label="" type="data_stream_select">
			<param type="function" value="ID_FUNCTION_READ_DATASTREAM"/>
			<param label="data_stream_id" type="ui_data_buffer" value=""/>
			<param label="GROUP" type="string" value="0"/>
		</show>
	</function>
	<function ID="ID_FUNCTION_READ_DATASTREAM">
		<call_protocol label="data_stream" function="read_data_stream">
			<param type="ui_data_buffer" value="data_stream_id"/>
		</call_protocol>
		<judge_tip_jump use_label="data_stream"/>
		<show label="" type="read_data_stream">
			<param type="protocol_data" value="data_stream"/>
		</show>
	</function>
	<data_stream_lib>
		<!-- 数据流 -->
		<group ID="0" caption="ID_STR_DS_GROUP_00">
			<!-- Wheel Speed FL -->
			<data_stream ID="0" caption="ID_STR_DS_ITEM_0000" unit="km/h" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_000</command_id>
				<valid_byte_offset>2</valid_byte_offset>
				<valid_byte_number>2</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*256+B)*0.0563</formula>
				</caculate>
				<!--显示控制-->
				<format>%.4f</format>
			</data_stream>
			<!-- Wheel Speed FR -->
			<data_stream ID="1" caption="ID_STR_DS_ITEM_0001" unit="km/h" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_000</command_id>
				<valid_byte_offset>4</valid_byte_offset>
				<valid_byte_number>2</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*256+B)*0.0563</formula>
				</caculate>
				<!--显示控制-->
				<format>%.4f</format>
			</data_stream>
			<!-- Wheel Speed RL -->
			<data_stream ID="2" caption="ID_STR_DS_ITEM_0002" unit="km/h" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_000</command_id>
				<valid_byte_offset>6</valid_byte_offset>
				<valid_byte_number>2</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*256+B)*0.0563</formula>
				</caculate>
				<!--显示控制-->
				<format>%.4f</format>
			</data_stream>
			<!-- Wheel Speed RR -->
			<data_stream ID="3" caption="ID_STR_DS_ITEM_0003" unit="km/h" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_000</command_id>
				<valid_byte_offset>8</valid_byte_offset>
				<valid_byte_number>2</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*256+B)*0.0563</formula>
				</caculate>
				<!--显示控制-->
				<format>%.4f</format>
			</data_stream>
			<!-- Vehicle Speed -->
			<data_stream ID="4" caption="ID_STR_DS_ITEM_0004" unit="km/h" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_001</command_id>
				<valid_byte_offset>2</valid_byte_offset>
				<valid_byte_number>2</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*256+B)*0.0563</formula>
				</caculate>
				<!--显示控制-->
				<format>%.4f</format>
			</data_stream>
			<!-- EVFL - Inlet valve front lefts -->
			<data_stream ID="5" caption="ID_STR_DS_ITEM_0005" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_002</command_id>
				<valid_byte_offset>2</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X1)Y=@0009;ELSE Y=@0008;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- AVFL - Outlet valve front left -->
			<data_stream ID="6" caption="ID_STR_DS_ITEM_0006" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_002</command_id>
				<valid_byte_offset>2</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X2)Y=@0009;ELSE Y=@0008;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- EVFR - Inlet valve front right -->
			<data_stream ID="7" caption="ID_STR_DS_ITEM_0007" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_002</command_id>
				<valid_byte_offset>2</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X4)Y=@0009;ELSE Y=@0008;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- AVFR - Outlet valve front right -->
			<data_stream ID="8" caption="ID_STR_DS_ITEM_0008" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_002</command_id>
				<valid_byte_offset>2</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X8)Y=@0009;ELSE Y=@0008;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- EVRL - Inlet valve rear left -->
			<data_stream ID="9" caption="ID_STR_DS_ITEM_0009" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_002</command_id>
				<valid_byte_offset>2</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X10)Y=@0009;ELSE Y=@0008;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- AVRL - Outlet valve rear left -->
			<data_stream ID="10" caption="ID_STR_DS_ITEM_0010" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_002</command_id>
				<valid_byte_offset>2</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X20)Y=@0009;ELSE Y=@0008;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- EVRR - Inlet valve rear right -->
			<data_stream ID="11" caption="ID_STR_DS_ITEM_0011" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_002</command_id>
				<valid_byte_offset>2</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X40)Y=@0009;ELSE Y=@0008;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- AVRR - Outlet valve rear right -->
			<data_stream ID="12" caption="ID_STR_DS_ITEM_0012" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_002</command_id>
				<valid_byte_offset>2</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X80)Y=@0009;ELSE Y=@0008;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- Pump motor status -->
			<data_stream ID="13" caption="ID_STR_DS_ITEM_0013" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_003</command_id>
				<valid_byte_offset>2</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>switch(0,A)</formula>
				</caculate>
				<!--显示控制-->
				<format>STRING</format>
			</data_stream>
			<!-- Battery voltage -->
			<data_stream ID="14" caption="ID_STR_DS_ITEM_0014" unit="V" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_004</command_id>
				<valid_byte_offset>2</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>A*0.08</formula>
				</caculate>
				<!--显示控制-->
				<format>%.2f</format>
			</data_stream>
		</group>
	</data_stream_lib>
	<switchs>
		<!-- 00:OFF FF:ON 其他:-  -->
		<switch ID="0">
			<case value="0x00">ID_DS_STRING_000</case>
			<case value="0xFF">ID_DS_STRING_001</case>
			<default>ID_DS_STRING_002</default>
		</switch>
	</switchs>
</root>
