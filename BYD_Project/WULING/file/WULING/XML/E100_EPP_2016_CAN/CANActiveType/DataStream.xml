<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<root version="1.0">
	<function ID="ID_FUNCTION_DATASTREAM">
		<!--选择数据流，并把选择的数据流ID保存到ui_data_buffer中-->
		<show label="" type="data_stream_select">
			<param type="function" value="ID_FUNCTION_READ_DATASTREAM"/>
			<param label="data_stream_id" type="ui_data_buffer" value=""/>
			<param label="GROUP" type="string" value="0"/>
		</show>
	</function>
	<function ID="ID_FUNCTION_READ_DATASTREAM">
		<call_protocol label="data_stream" function="read_data_stream">
			<param type="ui_data_buffer" value="data_stream_id"/>
		</call_protocol>
		<judge_tip_jump use_label="data_stream"/>
		<show label="" type="read_data_stream">
			<param type="protocol_data" value="data_stream"/>
		</show>
	</function>
	<data_stream_lib>
		<!-- 数据流 -->
		<group ID="0" caption="ID_STR_DS_GROUP_00">
			<!-- ECU供电电压 -->
			<data_stream ID="0" caption="ID_STR_DS_ITEM_0000" unit="V" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_000</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>A*0.1</formula>
				</caculate>
				<!--显示控制-->
				<format>%.1f</format>
			</data_stream>
			<!-- 车速 -->
			<data_stream ID="1" caption="ID_STR_DS_ITEM_0001" unit="km/h" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_001</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>2</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*256+B)*0.015625</formula>
				</caculate>
				<!--显示控制-->
				<format>%.6f</format>
			</data_stream>
			<!-- 车辆电机实际转速 -->
			<data_stream ID="2" caption="ID_STR_DS_ITEM_0002" unit="rpm" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_002</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>2</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*256+B)*1-16384</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- P档位置标定角度值 -->
			<data_stream ID="3" caption="ID_STR_DS_ITEM_0003" unit="°" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_003</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>2</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*256+B)*0.02197265625</formula>
				</caculate>
				<!--显示控制-->
				<format>%.11f</format>
			</data_stream>
			<!-- 非P档位置标定角度值 -->
			<data_stream ID="4" caption="ID_STR_DS_ITEM_0004" unit="°" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_003</command_id>
				<valid_byte_offset>5</valid_byte_offset>
				<valid_byte_number>2</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*256+B)*0.02197265625</formula>
				</caculate>
				<!--显示控制-->
				<format>%.11f</format>
			</data_stream>
			<!-- 位置传感器电压 -->
			<data_stream ID="5" caption="ID_STR_DS_ITEM_0005" unit="v" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_004</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>A*0.1</formula>
				</caculate>
				<!--显示控制-->
				<format>%.1f</format>
			</data_stream>
			<!-- 驻车锁状态 -->
			<data_stream ID="6" caption="ID_STR_DS_ITEM_0006" unit="0" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_005</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>switch(0,A)</formula>
				</caculate>
				<!--显示控制-->
				<format>STRING</format>
			</data_stream>
			<!-- 位置传感器实时值 -->
			<data_stream ID="7" caption="ID_STR_DS_ITEM_0007" unit="°" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_006</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>2</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*256+B)*0.02197265625</formula>
				</caculate>
				<!--显示控制-->
				<format>%.11f</format>
			</data_stream>
			<!-- EPB开关释放 -->
			<data_stream ID="8" caption="ID_STR_DS_ITEM_0008" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_007</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X1)Y=@0039;ELSE Y=@0038;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- EPB制动开关 -->
			<data_stream ID="9" caption="ID_STR_DS_ITEM_0009" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_008</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X1)Y=@0039;ELSE Y=@0038;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- EPB制动踏板状态 -->
			<data_stream ID="10" caption="ID_STR_DS_ITEM_0010" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_009</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X1)Y=@0039;ELSE Y=@0038;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- EPB加速踏板 -->
			<data_stream ID="11" caption="ID_STR_DS_ITEM_0011" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_010</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X1)Y=@0039;ELSE Y=@0038;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- EPB制动开关 -->
			<data_stream ID="12" caption="ID_STR_DS_ITEM_0012" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_011</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X8)Y=@0041;ELSE Y=@0040;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- EPB开关释放 -->
			<data_stream ID="13" caption="ID_STR_DS_ITEM_0013" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_011</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X4)Y=@0041;ELSE Y=@0040;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- 系统电机运行状态 -->
			<data_stream ID="14" caption="ID_STR_DS_ITEM_0014" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_011</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X2)Y=@0041;ELSE Y=@0040;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- 系统KL15状态 -->
			<data_stream ID="15" caption="ID_STR_DS_ITEM_0015" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_011</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="X117">
					<!--公式-->
					<formula>IF(X1&amp;0X1)Y=@0041;ELSE Y=@0040;</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
		</group>
	</data_stream_lib>
	<switchs>
		<!-- 00:Reserved(预留) 01:Parking(正在驻车) 02:Parked(驻车完成) 03:Park Stalled(驻车过程中出现堵转) 04:Unparking(正在解锁) 05:Unparked(解锁完成) 其他:-  -->
		<switch ID="0">
			<case value="0x00">ID_DS_STRING_000</case>
			<case value="0x01">ID_DS_STRING_001</case>
			<case value="0x02">ID_DS_STRING_002</case>
			<case value="0x03">ID_DS_STRING_003</case>
			<case value="0x04">ID_DS_STRING_004</case>
			<case value="0x05">ID_DS_STRING_005</case>
			<default>ID_DS_STRING_006</default>
		</switch>
	</switchs>
</root>
