<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<root version="1.0">
	<function ID="ID_FUNCTION_DATASTREAM">
		<!--选择数据流，并把选择的数据流ID保存到ui_data_buffer中-->
		<show label="" type="data_stream_select">
			<param type="function" value="ID_FUNCTION_READ_DATASTREAM"/>
			<param label="data_stream_id" type="ui_data_buffer" value=""/>
			<param label="GROUP" type="string" value="0,1"/>
		</show>
	</function>
	<function ID="ID_FUNCTION_READ_DATASTREAM">
		<call_protocol label="data_stream" function="read_data_stream">
			<param type="ui_data_buffer" value="data_stream_id"/>
		</call_protocol>
		<judge_tip_jump use_label="data_stream"/>
		<show label="" type="read_data_stream">
			<param type="protocol_data" value="data_stream"/>
		</show>
	</function>
	<data_stream_lib>
		<!-- Sensor ID -->
		<group ID="0" caption="ID_STR_DS_GROUP_00">
			<!-- LF sensor ID -->
			<data_stream ID="0" caption="ID_STR_DS_ITEM_0000" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_000</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>4</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*16777216+B*65536+C*256+D)</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- RF sensor ID -->
			<data_stream ID="1" caption="ID_STR_DS_ITEM_0001" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_000</command_id>
				<valid_byte_offset>7</valid_byte_offset>
				<valid_byte_number>4</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*16777216+B*65536+C*256+D)</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- LR sensor ID -->
			<data_stream ID="2" caption="ID_STR_DS_ITEM_0002" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_000</command_id>
				<valid_byte_offset>11</valid_byte_offset>
				<valid_byte_number>4</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*16777216+B*65536+C*256+D)</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
			<!-- RR sensor ID -->
			<data_stream ID="3" caption="ID_STR_DS_ITEM_0003" unit="-" min="" max="" self_learn="0">
				<command_id>DS_CMD_00_000</command_id>
				<valid_byte_offset>15</valid_byte_offset>
				<valid_byte_number>4</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>(A*16777216+B*65536+C*256+D)</formula>
				</caculate>
				<!--显示控制-->
				<format>%.0f</format>
			</data_stream>
		</group>
		<!-- Tire Pressure And Temperature -->
		<group ID="1" caption="ID_STR_DS_GROUP_01">
			<!-- LF tire pressure -->
			<data_stream ID="4" caption="ID_STR_DS_ITEM_0004" unit="kPa" min="" max="" self_learn="0">
				<command_id>DS_CMD_01_000</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>A*1.38</formula>
				</caculate>
				<!--显示控制-->
				<format>%.2f</format>
			</data_stream>
			<!-- RF tire pressure -->
			<data_stream ID="5" caption="ID_STR_DS_ITEM_0005" unit="kPa" min="" max="" self_learn="0">
				<command_id>DS_CMD_01_000</command_id>
				<valid_byte_offset>4</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>A*1.38</formula>
				</caculate>
				<!--显示控制-->
				<format>%.2f</format>
			</data_stream>
			<!-- LR tire pressure -->
			<data_stream ID="6" caption="ID_STR_DS_ITEM_0006" unit="kPa" min="" max="" self_learn="0">
				<command_id>DS_CMD_01_000</command_id>
				<valid_byte_offset>5</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>A*1.38</formula>
				</caculate>
				<!--显示控制-->
				<format>%.2f</format>
			</data_stream>
			<!-- RR tire pressure -->
			<data_stream ID="7" caption="ID_STR_DS_ITEM_0007" unit="kPa" min="" max="" self_learn="0">
				<command_id>DS_CMD_01_000</command_id>
				<valid_byte_offset>6</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>A*1.38</formula>
				</caculate>
				<!--显示控制-->
				<format>%.2f</format>
			</data_stream>
			<!-- LF tire temperature -->
			<data_stream ID="8" caption="ID_STR_DS_ITEM_0008" unit="℃" min="" max="" self_learn="0">
				<command_id>DS_CMD_01_001</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>A*0.65-40</formula>
				</caculate>
				<!--显示控制-->
				<format>%.2f</format>
			</data_stream>
			<!-- RF tire temperature -->
			<data_stream ID="9" caption="ID_STR_DS_ITEM_0009" unit="℃" min="" max="" self_learn="0">
				<command_id>DS_CMD_01_001</command_id>
				<valid_byte_offset>4</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>A*0.65-40</formula>
				</caculate>
				<!--显示控制-->
				<format>%.2f</format>
			</data_stream>
			<!-- LR tire temperature -->
			<data_stream ID="10" caption="ID_STR_DS_ITEM_0010" unit="℃" min="" max="" self_learn="0">
				<command_id>DS_CMD_01_001</command_id>
				<valid_byte_offset>5</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>A*0.65-40</formula>
				</caculate>
				<!--显示控制-->
				<format>%.2f</format>
			</data_stream>
			<!-- RR tire temperature -->
			<data_stream ID="11" caption="ID_STR_DS_ITEM_0011" unit="℃" min="" max="" self_learn="0">
				<command_id>DS_CMD_01_001</command_id>
				<valid_byte_offset>6</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>A*0.65-40</formula>
				</caculate>
				<!--显示控制-->
				<format>%.2f</format>
			</data_stream>
			<!-- ECU input voltage -->
			<data_stream ID="12" caption="ID_STR_DS_ITEM_0012" unit="V" min="" max="" self_learn="0">
				<command_id>DS_CMD_01_002</command_id>
				<valid_byte_offset>3</valid_byte_offset>
				<valid_byte_number>1</valid_byte_number>
				<caculate type="FORMULA_PARSER">
					<!--公式-->
					<formula>A*0.001</formula>
				</caculate>
				<!--显示控制-->
				<format>%.3f</format>
			</data_stream>
		</group>
	</data_stream_lib>
	<switchs/>
</root>
